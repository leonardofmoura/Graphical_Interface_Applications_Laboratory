        </component>

        <!-- Candeeiro por cima da mesa de jogo-->
        <!-- Cabo do Candeeiro-->
        <component id ="caboCandeeiro">
            <transformation>
                <translate x="0" y="20" z="0" />
                <rotate axis="x" angle="90"/>
                <scale x="0.1" y="0.1" z="8"/>
            </transformation>

            <materials>
                <material id="bluePlastic" />
            </materials>

            <texture id="inherit"/>

            <children>
                <componentref id="CilindroFechado"/>
            </children>
        </component>

        <component id ="abatjourcandeeiroalto">
            <transformation>
                <translate x="0" y ="10" z="0"/>
                <rotate axis="x" angle="-90"/>
                <scale x="1" y="1" z="1"/>


            </transformation>

            <materials>
                <material id="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="cone"/>
            </children>
        </component>



        <component id ="candeeiro">
            <transformation>

            </transformation>

            <materials>
                <material id="bluePlastic" />
            </materials>

            <texture id="none"/>

            <children>
                <componentref id="abatjourcandeeiroalto"/>
                <componentref id="caboCandeeiro"/>
            </children>
        </component>

        <component id ="gameboard">
            <transformation>
                <translate x="0" y ="4.4" z="0"/>
            </transformation>

            <materials>
                <material id="bluePlastic"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="gameboard"/>
            </children>
        </component>

        <!-- Boneco de Neve-->
        <component id ="cabecaNeve">
            <transformation>
                <translate x="0" y="14" z="0" />
                <scale x="2" y="2" z="2" />
